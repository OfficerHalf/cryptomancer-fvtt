<form class="crypt-actor-sheet party" autocomplete="off">
  <header></header>
  <nav class="sheet-tabs tabs" data-group="primary">
    <a class="item" data-tab="party">
      {{localize "CRYPTOMANCER.Party.party"}}
    </a>
    <a class="item" data-tab="safehouse">
      {{localize "CRYPTOMANCER.Party.safehouse"}}
    </a>
  </nav>
  <section class="sheet-body">
    <div class="tab party" data-group="primary" data-tab="party">
    </div>
    <div class="tab safehouse" data-group="primary" data-tab="safehouse">
      <div class="safehouse-rooms">
        {{> "systems/cryptomancer/actor-sheet/components/safehouse-room.hbs" room=data.data.safehouse.cryptovault}}
        {{> "systems/cryptomancer/actor-sheet/components/safehouse-room.hbs" room=data.data.safehouse.dungeon}}
        {{> "systems/cryptomancer/actor-sheet/components/safehouse-room.hbs" room=data.data.safehouse.forge}}
        {{> "systems/cryptomancer/actor-sheet/components/safehouse-room.hbs" room=data.data.safehouse.front}}
        {{> "systems/cryptomancer/actor-sheet/components/safehouse-room.hbs" room=data.data.safehouse.golem}}
        {{> "systems/cryptomancer/actor-sheet/components/safehouse-room.hbs" room=data.data.safehouse.laboratory}}
        {{> "systems/cryptomancer/actor-sheet/components/safehouse-room.hbs" room=data.data.safehouse.lounge}}
        {{> "systems/cryptomancer/actor-sheet/components/safehouse-room.hbs" room=data.data.safehouse.sanctuary}}
        {{> "systems/cryptomancer/actor-sheet/components/safehouse-room.hbs" room=data.data.safehouse.stable}}
        {{> "systems/cryptomancer/actor-sheet/components/safehouse-room.hbs" room=data.data.safehouse.trainingRoom}}
        {{> "systems/cryptomancer/actor-sheet/components/safehouse-room.hbs" room=data.data.safehouse.warRoom}}
      </div>
    </div>
  </section>
</form>